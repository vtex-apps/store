{
  "store": {
    "allowConditions": true,
    "component": "*",
    "around": ["storeWrapper", "challenge", "offlineWrapper"],
    "before": ["header.full"],
    "after": ["footer", "pixels"],
    "allowed": [
      "notification.bar",
      "carousel",
      "shelf",
      "rebuy",
      "promo-bar",
      "info-card",
      "newsletter",
      "rich-text",
      "flex-layout",
      "sandbox",
      "image",
      "sticky-layout"
    ],
    "preview": {
      "type": "transparent",
      "height": 600
    }
  },
  "store.home": {
    "around": ["homeWrapper"],
    "allowed": ["search-result"],
    "preview": {
      "type": "box",
      "height": 1400
    }
  },
  "store.product": {
    "around": ["productWrapper"],
    "before": ["header.full"],
    "allowed": [
      "product-add-to-list-button",
      "product-details",
      "product-kit",
      "product-reviews",
      "product-questions-and-answers",
      "product-rating-summary",
      "product-rating-inline",
      "product-images",
      "product-brand",
      "product-name",
      "product-price",
      "sku-selector",
      "buy-button",
      "product-separator",
      "product-description",
      "product-specifications",
      "product-quantity",
      "breadcrumb",
      "shipping-simulator",
      "availability-subscriber",
      "share",
      "product-highlights",
      "product-availability",
      "product-identifier",
      "blog-related-posts"
    ],
    "context": "ProductContext",
    "preview": {
      "type": "box",
      "height": 800
    }
  },
  "store.search": {
    "around": ["searchWrapper"],
    "required": ["search-result"],
    "context": "SearchContext",
    "preview": {
      "type": "grid",
      "height": 1400
    }
  },
  "store.account": {
    "required": ["my-account"],
    "around": ["challenge.profile"],
    "preview": {
      "type": "text",
      "height": 800
    }
  },
  "store.login": {
    "required": ["login-content"],
    "preview": {
      "type": "spinner",
      "height": 600
    }
  },
  "store.orderplaced": {
    "required": ["order-placed"],
    "allowed": ["shop-review-form"],
    "preview": {
      "type": "spinner",
      "height": 600
    }
  },
  "store.product-review-form": {
    "allowed": ["product-review-form"],
    "preview": {
      "type": "spinner",
      "height": 800
    }
  },
  "store.blog-home": {
    "before": ["header.full"],
    "allowed": [
      "blog-all-posts",
      "blog-latest-posts-preview",
      "blog-category-preview",
      "blog-search"
    ]
  },
  "store.blog-category": {
    "before": ["header.full"],
    "required": [
      "blog-category-list"
    ],
    "allowed": [
      "blog-search",
      "blog-category-preview"
    ]
  },
  "store.blog-post": {
    "before": ["header.full"],
    "required": [
      "blog-post-details"
    ],
    "allowed": [
      "blog-search"
    ]
  },
  "store.blog-search-result": {
    "before": ["header.full"],
    "required": [
      "blog-search-list"
    ],
    "allowed": [
      "blog-search"
    ]
  },
  "storeWrapper": {
    "allowed": ["highlight-overlay"],
    "component": "StoreWrapper"
  },
  "offlineWrapper": {
    "before": ["header.full"],
    "component": "OfflineWrapper",
    "required": ["offline-warning"]
  },
  "homeWrapper": {
    "component": "HomeWrapper"
  },
  "productWrapper": {
    "component": "ProductWrapper"
  },
  "searchWrapper": {
    "component": "SearchWrapper"
  },
  "customWrapper": {
    "component": "CustomWrapper"
  },
  "challenge": {
    "component": "DefaultChallenge",
    "extensible": "public"
  },
  "highlight-overlay": {
    "component": "*",
    "extensible": "vtex"
  },
  "challenge.profile": {
    "component": "ProfileChallenge"
  },
  "promo-bar": {
    "component": "*"
  },
  "store.custom": {
    "allowed": ["search-result"]
  },
  "store.offline": {
    "required": ["offline-warning"]
  },
  "offline-warning": {
    "component": "OfflineWarning"
  }
}
