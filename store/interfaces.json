{
  "store": {
    "before": [
      "header.full"
    ],
    "around": [
      "store-wrapper"
    ],
    "after": [
      "footer"
    ],
    "allowed": [
      "carousel",
      "shelf",
      "rebuy",
      "address-manager"
    ]
  },
  "store.home": {
    "around": [
      "home-wrapper"
    ],
    "allowed": [
      "search-result"
    ]
  },
  "store.product": {
    "around": [
      "product-wrapper"
    ],
    "required": [
      "product-details"
    ],
    "allowed": [
      "product-kit",
      "related-products"
    ],
    "context": "ProductContext"
  },
  "store.search": {
    "around": [
      "search-wrapper"
    ],
    "required": [
      "search-result"
    ],
    "context": "SearchContext"
  },
  "store.account": {
    "required": [
      "my-account"
    ]
  },
  "store.login": {
    "required": [
      "login-content"
    ]
  },
  "store-wrapper": {
    "component": "StoreWrapper"
  },
  "home-wrapper": {
    "component": "HomeWrapper"
  },
  "product-wrapper": {
    "component": "ProductWrapper"
  },
  "search-wrapper": {
    "component": "SearchWrapper"
  }
}