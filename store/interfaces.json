{
  "store": {
    "component": "*",
    "around": [
      "storeWrapper",
      "challenge"
    ],
    "before": [
      "header"
    ],
    "after": [
      "footer"
    ],
    "allowed": [
      "carousel",
      "shelf",
      "rebuy",
      "address-manager",
      "promo-banner"
    ],
    "preview": {
      "type": "block"
    }
  },
  "store.home": {
    "around": [
      "homeWrapper"
    ],
    "before": [
      "header.full"
    ],
    "allowed": [
      "search-result"
    ]
  },
  "store.product": {
    "around": [
      "productWrapper"
    ],
    "before": [
      "header.full"
    ],
    "required": [
      "product-details"
    ],
    "allowed": [
      "product-kit",
      "related-products"
    ],
    "context": "ProductContext"
  },
  "store.search": {
    "around": [
      "searchWrapper"
    ],
    "before": [
      "header.full"
    ],
    "required": [
      "search-result"
    ],
    "context": "SearchContext"
  },
  "store.account": {
    "before": [
      "header.full"
    ],
    "required": [
      "my-account"
    ]
  },
  "store.login": {
    "before": [
      "header.full"
    ],
    "required": [
      "login-content"
    ]
  },
  "storeWrapper": {
    "allowed": ["highlight-overlay"],
    "component": "StoreWrapper"
  },
  "homeWrapper": {
    "component": "HomeWrapper"
  },
  "productWrapper": {
    "component": "ProductWrapper"
  },
  "searchWrapper": {
    "component": "SearchWrapper"
  },
  "challenge": {
    "component": "DefaultChallenge",
    "extensible": "public"
  },
  "highlight-overlay": {
    "component": "*",
    "extensible": "vtex"
  },
  "challenge.profile": {
    "component": "ProfileChallenge"
  }
}
